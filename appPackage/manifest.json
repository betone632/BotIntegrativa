{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.23/MicrosoftTeams.schema.json",
    "manifestVersion": "1.23",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "My App, Inc.",
        "websiteUrl": "https://www.example.com",
        "privacyUrl": "https://www.example.com/privacy",
        "termsOfUseUrl": "https://www.example.com/termofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "BotIntegrativa${{APP_NAME_SUFFIX}}",
        "full": "full name for BotIntegrativa"
    },
    "description": {
        "short": "short description for BotIntegrativa",
        "full": "full description for BotIntegrativa"
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupChat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false,
            "commandLists": [
                {
                    "scopes": [
                        "personal",
                        "team",
                        "groupChat"
                    ],
                    "commands": [
                        {
                            "title": "Hi",
                            "description": "Say hi to the bot."
                        },
                        {
                            "title": "/reset",
                            "description": "Delete conversation state."
                        },
                        {
                            "title": "/reuni천es",
                            "description": "Lista as as reuni천es."
                        },
                        {
                            "title": "/resumir reuni찾o",
                            "description": "Resume a reuni찾o do chat atual."
                        }
                    ]
                }
            ]
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}